pipeline{
    agent any
    stages{
        stage("checkout code"){
            steps{
                  git url : "https://github.com/Arvind-kumar2006/DevOps",branch:'main'
            }
          
        }
        stage("Build"){
            steps{
                echo "Build"
            }
        }
        stage('test'){
            steps{
                echo "Test"
            }
        }
        stage("Release"){
            steps{
                echo "Release"
                
            }
        }
    }
    
}